    "## =============================================\n",
    "## 1) reshape(): 총 원소수 보존, 차원/형태만 변경\n",
    "## =============================================\n",
    "## => shape 지정\n",
    "b = arr.reshape(1, 6)           #\n",
    "c = arr.reshape(-1)\n",
    "d = arr.reshape(1, 2, 3, 1)\n",
    "e = arr.reshape(-1, 1)\n",
    "\n",
    "print(\"[reshape] ------\")\n",
    "print( b, b.shape)\n",
    "print( c, c.shape)\n",
    "print( d, d.shape)\n",
    "print( e, e.shape)\n",
    "\n",
    "\n",
    "# 2) -1(미지정 차원) 자동 계산\n",
    "e = np.arange(24).reshape(2, 3, 4)\n",
    "print(e)\n",
    "print(\"[-1 사용]\")\n",
    "print( e.reshape(6, -1).shape )\n",
    "print( e.reshape(-1, 8).shape )"
   ]
