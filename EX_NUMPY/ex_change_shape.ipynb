    "## ================================================\n",
    "## 2) flatten() vs ravel(): 펼치기 (copy vs view)\n",
    "## ================================================\n",
    "f1 = arr.flatten()  # 복사(copy) -> 원본과 별개\n",
    "f2 = arr.ravel()    # 가급적 view(공유) -> 원본 변경의 영향 가능\n",
    "\n",
    "print('원본 - 변경전', arr, sep='\\n')\n",
    "\n",
    "## 원소값 변경\n",
    "arr[0,0] = 999\n",
    "\n",
    "print('원본 - 변경후', arr, sep='\\n')\n",
    "print('복사본 - f1',   f1, sep='\\n')\n",
    "print('공유본 - f2',   f2, sep='\\n')\n"
   ]
